# NOTE: this file is processed with sed, replacing <DPL> and <REGION>
terraform {
  backend "s3" {
    bucket         = "<DPL>-tfstate"         # This must be in sync with aws_s3_bucket.tfstate_bucket
    key            = "<DPL>-tfstate-install"

    dynamodb_table = "<DPL>-tfstate-install" # This must be in sync with aws_dynamodb_table.dynamodb_tfstate_lock

    region         = "<REGION>"
    encrypt        = true
  }
}
